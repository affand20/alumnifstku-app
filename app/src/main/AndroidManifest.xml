<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.trydev.alumnifstku">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fst"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_fst_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlumniFSTku">
        <activity android:name=".ui.trace.TracingActivity"></activity>
        <activity android:name=".ui.splash.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.dashboard.DashboardActivity" />
        <activity
            android:name=".ui.splash.SplashActivity"
            android:noHistory="true" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>